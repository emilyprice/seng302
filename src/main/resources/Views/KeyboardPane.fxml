<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import com.jfoenix.controls.JFXButton?>
<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<TitledPane fx:id="keyPane" contentDisplay="GRAPHIC_ONLY" focusTraversable="false" maxHeight="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seng302.gui.KeyboardPaneController">
   <content>
       <StackPane fx:id="keyboardStack" alignment="TOP_LEFT" focusTraversable="true" maxHeight="100.0" prefHeight="0.0" styleClass="keyboardSplitpane">
          <padding>
              <Insets bottom="0" left="0" right="0" top="0" />
          </padding>
         <children>
             <HBox fx:id="keyboardBox" prefHeight="100.0" snapToPixel="false">
               <StackPane.margin>
                   <Insets right="-0.0" />
               </StackPane.margin>
            </HBox>
             <AnchorPane fx:id="blackKeys" minWidth="-Infinity" pickOnBounds="false" prefHeight="200.0" prefWidth="200.0" snapToPixel="false" StackPane.alignment="TOP_CENTER" />
         </children>
      </StackPane>
   </content>
   <graphic>
       <HBox alignment="CENTER" fillHeight="false" maxHeight="24.0" maxWidth="1.7976931348623157E308" prefHeight="24.0" spacing="6.0" styleClass="primary">
         <children>
             <StackPane fx:id="titlePaneKeyboard" alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" nodeOrientation="LEFT_TO_RIGHT" prefHeight="20.0" styleClass="primary" AnchorPane.leftAnchor="70.0" AnchorPane.topAnchor="13.0">
               <children>
                   <Label focusTraversable="false" prefHeight="9.0" prefWidth="62.0" styleClass="primary" text="Keyboard" StackPane.alignment="CENTER">
                       <StackPane.margin>
                           <Insets right="20.0" />
                       </StackPane.margin>
                   </Label>
               </children>
            </StackPane>
             <StackPane fx:id="rightStack" alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" pickOnBounds="false" prefHeight="48.0" styleClass="primary">
               <children>
                   <JFXButton fx:id="settingsButton" focusTraversable="false" mnemonicParsing="false" onAction="#toggleSettings" prefHeight="23.0" prefWidth="23.0" styleClass="keyboardButton" text="Settings">
                       <StackPane.margin>
                           <Insets right="5.0" />
                       </StackPane.margin>
                   </JFXButton>
               </children>
            </StackPane>
             <StackPane fx:id="scalesStackPane" alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" nodeOrientation="LEFT_TO_RIGHT" pickOnBounds="false" prefHeight="16.0" styleClass="primary">
                 <children>
                     <JFXButton fx:id="displayScalesButton" focusTraversable="false" mnemonicParsing="false" onAction="#toggleDisplayScales" prefHeight="23.0" prefWidth="150.0" styleClass="keyboardButton" text="Display Scales" />
                 </children>
                 <HBox.margin>
                     <Insets bottom="3.0" top="3.0" />
                 </HBox.margin>
             </StackPane>
         </children>
      </HBox>
   </graphic>
   <padding>
      <Insets bottom="2.0" top="2.0" />
   </padding>
   <styleClass>
      <String fx:value="primary" />
      <String fx:value="keyboardSplitpane" />
   </styleClass>
</TitledPane>
