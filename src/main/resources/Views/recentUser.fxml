<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seng302.gui.RecentUserController">
   <children>
      <StackPane VBox.vgrow="ALWAYS">
         <children>
            <ImageView fx:id="userPic" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" StackPane.alignment="CENTER">
               <image>
                  <Image url="@../images/stop.png" />
               </image>
            </ImageView>
         </children>
      </StackPane>
      <StackPane VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="username" text="Name" StackPane.alignment="CENTER" />
         </children>
      </StackPane>
   </children>
</VBox>
